generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = "file:./dev.db"
}

model User {
  id          Int         @id @default(autoincrement())
  email       String      @unique
  cpf         String      @unique
  phone       String      @unique
  name        String
  password    String     
  role        String    
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
}
